{
    "seeds": [
        {
            "https://www.usal.es": 2
        },
        {
            "https://www.unileon.es": 2
        }
    ],
    "robots": true,
    "recursive": true,
    "data": {
        "metadata": true,
        "links": true,
        "text": true,
        "images": true
    },
    "brokers": [
        "kafka-broker:9092"
    ],
    "producer-topic": "crawled_data",
    "retry-delays": [
        1000,
        2000,
        5000,
        10000,
        20000
    ],
    "timeout": 3.2,
    "workers": 30,
    "expiry-hours": 24,
    "chromium-bin-path": "/usr/bin/chromium",
    "req-per-second": 100,
    "max-bytes": 52428800
}